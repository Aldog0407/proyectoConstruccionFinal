<AnchorPane id="AnchorPane" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="practicasprofesionalespf.controller.teacher.FXMLConsultRecordController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="102.0" layoutX="34.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true">
         <image><Image url="@../../images/uv_logo.png" /></image>
      </ImageView>
      <Pane layoutX="730.0" prefHeight="56.0" prefWidth="294.0" style="-fx-background-color: #015aaa;">
         <children>
            <Label layoutX="12.0" layoutY="10.0" text="Universidad Veracruzana" textFill="WHITE">
               <font><Font name="Avenir Medium" size="24.0" /></font>
            </Label>
         </children>
      </Pane>
      <Label fx:id="lbTitle" layoutX="185.0" layoutY="118.0" text="Expediente del Estudiante">
         <font><Font name="Avenir Medium" size="36.0" /></font>
      </Label>
      <TableView fx:id="tvDocuments" layoutX="100.0" layoutY="200.0" prefHeight="450.0" prefWidth="824.0">
         <columns>
            <TableColumn fx:id="tcDocumentName" prefWidth="400.0" text="Nombre del Documento" />
