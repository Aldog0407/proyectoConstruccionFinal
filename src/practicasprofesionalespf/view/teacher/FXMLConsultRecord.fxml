      <Button fx:id="btnShow" layoutX="844.0" layoutY="680.0" mnemonicParsing="false" onAction="#btnShowClicked" prefHeight="40.0" prefWidth="120.0" style="-fx-background-color: #01ab4b;" text="Mostrar" textFill="WHITE">
